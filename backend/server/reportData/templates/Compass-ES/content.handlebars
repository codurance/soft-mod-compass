  <!-- BEGIN EFFECTIVE TEAM INTRO ---------------------------------------------------------- -->
   <section id="team-effectiveness-page-es" class="page page-break-after with-background-img"></section>
  <!-- END EFFECTIVE TEAM INTRO ---------------------------------------------------------- -->

  <!-- BEGIN EFFECTIVE TEAM RESULTS ------------------------------------------------------------ -->
  <section id="team-effectiveness-results-page-es" class="page page-break-after with-background-img">
    {{> gauge-subcategory
        id="gauge-crossfunctionalteams-diversity-es"
        subcategory=categories.crossFunctionalTeams.subcategories.diversity
