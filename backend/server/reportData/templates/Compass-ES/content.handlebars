  <!-- BEGIN ORGANISATIONAL MATURITY RESULTS -------------------------------------------------------- -->
  <section id="organisational-maturity-results-page-es" class="page page-break-after with-background-img">
    {{> gauge-subcategory
        id="gauge-organisationalmaturity-devsecops-es"
        subcategory=categories.organisationalMaturity.subcategories.devSecOps
        lang="es"
        onDark=true }}

    {{> gauge-subcategory
        id="gauge-organisationalmaturity-deliveringvalue-es"
        subcategory=categories.organisationalMaturity.subcategories.deliveringValue
        lang="es"
        onDark=true }}

    {{> gauge-subcategory
        id="gauge-organisationalmaturity-technicaldebt-es"
        subcategory=categories.organisationalMaturity.subcategories.technicalDebt
        lang="es"
        onDark=true }}

    {{> gauge-subcategory
        id="gauge-organisationalmaturity-methodology-es"
        subcategory=categories.organisationalMaturity.subcategories.methodology
        lang="es"
        onDark=true }}
  </section>
  <!---- END ORGANISATIONAL MATURITY RESULTS -------------------------------------------------------- -->

  <!-- BEGIN ORGANISATIONAL MATURITY BREAKDOWN ---------------------------------------------------------- -->
  <section
    id="organisational-maturity-breakdown-page-es"
    class="page page-break-after with-background-img {{{categoryScoreClass categories.organisationalMaturity.score}}}"></section>
  <!-- END ORGANISATIONAL MATURITY BREAKDOWN ---------------------------------------------------------- -->



  <!-- BEGIN CONTINUOUS DELIVERY INTRO ---------------------------------------------------------- -->
