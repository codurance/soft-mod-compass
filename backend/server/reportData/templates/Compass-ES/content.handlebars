<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <!-- Proxima Nova font from Typekit -->
  <link rel="stylesheet" href="https://use.typekit.net/gpw7xtk.css">
  <title>Codurance Compass</title>
  <style>
    {#asset /css/main.css @encoding=utf8}
    {#asset /css/background-imgs-es.css @encoding=utf8}
  </style>
</head>
<body>



  <!-- BEGIN FRONT PAGE ---------------------------------------------------------- -->
  <section id="front-page-es" class="page page-break-after with-background-img">
    <span id="front-page_full-name">{{user.firstName}} {{user.lastName}}</span>
    <span id="front-page_company-name">{{user.company}}</span>
    <span id="front-page_date">{{ currentDate }}</span>
  </section>
  <!-- END FRONT PAGE ---------------------------------------------------------- -->

  <!-- BEGIN INTRO PAGE ---------------------------------------------------------- -->
  <section id="intro-page-es" class="page page-break-after with-background-img">
    <ul class="intro-page__contact-info">
        <li class="intro-page__contact-header">
            Londres / Manchester
        </li>
      <li>
          Teléfono: <a href="tel:+442074902967">+44 207 490 2967</a>
      </li>
      <li>
          Email: <a href="mailto:david.hall@codurance.com">david.hall@codurance.com</a>
      </li>
    </ul>
    <ul class="intro-page__contact-info second">
        <li class="intro-page__contact-header">
            Barcelona
        </li>
        <li>
            Teléfono:: <a href="tel:+34937822882">+34 937 82 28 82</a>
        </li>
        <li>
            Email: <a href="mailto:jack.coleman@codurance.com">jack.coleman@codurance.com</a>
        </li>
    </ul>
  </section>
  <!-- END INTRO PAGE ---------------------------------------------------------- -->

  <!-- BEGIN YOUR ASSESSMENT PAGE ---------------------------------------------------------- -->
  <section id="assessment-page-es" class="page page-break-after with-background-img">
    {{> gauge-assessment
        score=categories.organisationalMaturity.score
        category="organisational-maturity"
        lang="es" }}

    {{> gauge-assessment
        score=categories.continuousDelivery.score
        category="continuous-delivery"
        lang="es" }}

    {{> gauge-assessment
        score=categories.culture.score
        category="organisational-culture"
        lang="es" }}

    {{> gauge-assessment
        score=categories.crossFunctionalTeams.score
        category="team-effectiveness"
        lang="es" }}

    {{> gauge-assessment
        score=categories.xpPractices.score
        category="xp-practices"
        lang="es" }}

  </section>
  <!-- END YOUR ASSESSMENT PAGE ---------------------------------------------------------- -->

  <!-- BEGIN TOC PAGE ---------------------------------------------------------- -->
  <section id="toc-page-es" class="page page-break-after with-background-img">

      <ul class="toc-page__contents">
          <a href="#organisational-maturity-page-es"><li>
            <span class="toc-page__numbers">4.</span>
            <span class="toc-page__sections">Madurez organizacional</span>
          </li></a>
          <li><a href="#continuous-delivery-page-es">
            <span class="toc-page__numbers">7.</span>
            <span class="toc-page__sections">Entrega continua</span>
          </a></li>
