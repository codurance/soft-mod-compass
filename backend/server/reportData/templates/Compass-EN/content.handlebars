    {{> gauge-subcategory 
        id="gauge-culture-careerpath-en"
        subcategory=categories.culture.subcategories.careerPath
        lang="en"
        onDark=true }}
  </section>
  <!---- END PEOPLE CULTURE RESULTS ------------------------------------------------------------ -->

  <!-- BEGIN PEOPLE CULTURE BREAKDOWN ---------------------------------------------------------- -->
  <section
    id="people-culture-breakdown-page-en"
    class="page page-break-after with-background-img {{{categoryScoreClass categories.culture.score}}}"></section>
  <!-- END PEOPLE CULTURE BREAKDOWN ---------------------------------------------------------- -->



  <!-- BEGIN EFFECTIVE TEAM INTRO ---------------------------------------------------------- -->
   <section id="effective-team-page-en" class="page page-break-after with-background-img"></section>
  <!-- END EFFECTIVE TEAM INTRO ---------------------------------------------------------- -->

  <!-- BEGIN EFFECTIVE TEAM RESULTS ------------------------------------------------------------ -->
  <section id="effective-team-results-page-en" class="page page-break-after with-background-img">
    {{> gauge-subcategory 
        id="gauge-crossfunctionalteams-diversity-en"
        subcategory=categories.crossFunctionalTeams.subcategories.diversity
        lang="en"
        onDark=false }}

    {{> gauge-subcategory 
        id="gauge-crossfunctionalteams-autonomy-en"
        subcategory=categories.crossFunctionalTeams.subcategories.autonomy
        lang="en"
        onDark=false }}

    {{> gauge-subcategory 
        id="gauge-crossfunctionalteams-wholeteam-en"
        subcategory=categories.crossFunctionalTeams.subcategories.wholeTeam
