  <!-- BEGIN TECHNICAL PRACTICES BREAKDOWN ---------------------------------------------------------- -->
  <section
    id="technical-practices-breakdown-page-en"
    class="page page-break-after with-background-img {{{categoryScoreClass categories.xpPractices.score}}}"></section>
  <!-- END TECHNICAL PRACTICES BREAKDOWN ---------------------------------------------------------- -->



  <!-- BEGIN SUMMARY ---------------------------------------------------------- -->
  <section id="summary-page-en" class="page page-break-after with-background-img">
      
    <ul class="summary-contact__london-container">
        <li>Sales Contact: <span class="summary-contact__name">David Hall</span></li>
        <li>
