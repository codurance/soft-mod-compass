                <li><a href="tel:+34937822882">
                    <i class="summary-contact__phone"></i>
                    <span>+34 937 82 28 82</span>
                </a></li>
                <li><a href="mailto:jack.coleman@codurance.com">
                    <i class="summary-contact__email"></i>
                    <span>jack.coleman@codurance.com</span>
                </a></li>
                <li><a href="https://codurance.es/" target="_blank">
                    <i class="summary-contact__web"></i>
                    <span>codurance.es</span>
                </a></li>
            </ul>
        </li>
        <li>Carrer de Pallars 99,<br>4th floor, room 41<br>Barcelona, 08018</li>
    </ul>

    {{> gauge-summary 
        score=categories.organisationalMaturity.score
        category="organisational-maturity"
        lang="en" }}
        
    {{> gauge-summary 
        score=categories.continuousDelivery.score
        category="continuous-delivery"
        lang="en" }}
        
    {{> gauge-summary 
        score=categories.culture.score
        category="people-culture"
        lang="en" }}
        
    {{> gauge-summary 
        score=categories.crossFunctionalTeams.score
        category="effective-team"
        lang="en" }}
        
    {{> gauge-summary 
        score=categories.xpPractices.score
