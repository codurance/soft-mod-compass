        onDark=false }}

    {{> gauge-subcategory 
        id="gauge-crossfunctionalteams-knowledgesharing-en"
        subcategory=categories.crossFunctionalTeams.subcategories.knowledgeSharing
        lang="en"
        onDark=false }}
  </section>
  <!---- END EFFECTIVE TEAM RESULTS ------------------------------------------------------------ -->

  <!-- BEGIN EFFECTIVE TEAM BREAKDOWN ---------------------------------------------------------- -->
  <section
    id="effective-team-breakdown-page-en"
    class="page page-break-after with-background-img {{{categoryScoreClass categories.crossFunctionalTeams.score}}}"></section>
  <!-- END EFFECTIVE TEAM BREAKDOWN ---------------------------------------------------------- -->



  <!-- BEGIN TECHNICAL PRACTICES INTRO ---------------------------------------------------------- -->
  <section id="technical-practices-page-en" class="page page-break-after with-background-img"></section>
  <!-- END TECHNICAL PRACTICES INTRO ---------------------------------------------------------- -->

  <!-- BEGIN TECHNICAL PRACTICES RESULTS ------------------------------------------------------- -->
  <section id="technical-practices-results-page-en" class="page page-break-after with-background-img">
    {{> gauge-subcategory 
        id="gauge-xppractices-tdd-en"
        subcategory=categories.xpPractices.subcategories.tdd
        lang="en"
        onDark=true }}

    {{> gauge-subcategory 
        id="gauge-xppractices-architecture-en"
