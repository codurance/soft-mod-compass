{
  "name": "codurance-compass",
  "version": "0.0.1",
  "main": "./server/index.js",
  "scripts": {
    "dev": ". ./scripts/localConf.sh && babel-node ./server/index.js",
    "dev:es": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-ES.sh && babel-node ./server/index.js",
    "debug:en": "node --inspect ./server/index.js",
    "debug:es": ". ./scripts/envvars-config-dev-ES.sh &&  node --inspect ./server/index.js",
    "prod:en": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-EN.sh && babel-node ./server/index.js",
    "prod:es": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-ES.sh && babel-node ./server/index.js",
    "watch": "nodemon",
    "test": "jest",
    "test:tdd": "jest --watch",
    "test:e2e:dev:en": ". ./scripts/envvars-config-dev-EN.sh && cypress run --project testE2E",
    "test:e2e:dev:es": ". ./scripts/envvars-config-dev-ES.sh && cypress run --project testE2E",
    "test:e2eOpen:dev:en": ". ./scripts/envvars-config-dev-EN.sh && cypress open --project testE2E",
    "test:e2eOpen:dev:es": ". ./scripts/envvars-config-dev-ES.sh && cypress open --project testE2E",
    "lint": "standard",
    "check-aws-region:prod": "./scripts/check-aws-region.sh eu-central-1",
    "check-aws-region:dev": "./scripts/check-aws-region.sh eu-west-1",
    "create-env:dev:en": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-EN.sh && ./scripts/aws/create-env.sh dev-en",
    "create-env:dev:es": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-ES.sh && ./scripts/aws/create-env.sh dev-es",
    "create-env:prod:en": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-EN.sh && ./scripts/aws/create-env.sh prod-en",
    "create-env:prod:es": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-ES.sh && ./scripts/aws/create-env.sh prod-es",
    "destroy-env:dev:en": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-EN.sh && ./scripts/aws/destroy-env.sh dev-en",
    "destroy-env:dev:es": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-ES.sh && ./scripts/aws/destroy-env.sh dev-es",
    "destroy-env:prod:en": "npm run check-aws-region:prod && ./scripts/aws/destroy-env.sh prod-en",
    "destroy-env:prod:es": "npm run check-aws-region:prod && ./scripts/aws/destroy-env.sh prod-es",
    "deploy:dev:en": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-EN.sh && ./scripts/aws/deploy.sh dev-en",
    "deploy:dev:es": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-ES.sh && ./scripts/aws/deploy.sh dev-es",
    "deploy:prod:en": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-EN.sh && ./scripts/aws/deploy.sh prod-en",
    "deploy:prod:es": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-ES.sh && ./scripts/aws/deploy.sh prod-es"
  },
  "author": "Codurance",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@hubspot/cms-cli": "^1.1.8",
    "jest-dynalite": "^3.3.1",
    "@typeform/embed": "^0.22.0",
    "aws-sdk": "^2.638.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "4.16.4",
    "jsreport": "2.4.0",
    "request": "^2.88.2",
    "request-promise": "4.2.2",
    "sleep-promise": "8.0.1",
    "uuid": "3.3.2",
    "uuidgen": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@types/jest": "^26.0.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cypress": "^4.7.0",
    "cypress-iframe": "^1.0.1",
    "cypress-wait-until": "^1.7.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "26.4.2",
    "nock": "10.0.2",
    "nodemon": "1.18.10",
    "pdf2pic": "^2.1.2",
    "pixelmatch": "^5.2.1",
    "prettier": "2.0.5",
    "react-dev-utils": "^10.2.1",
    "request-debug": "^0.2.0",
    "resemblejs": "^3.2.5",
    "standard": "12.0.1",
    "supertest": "3.3.0",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "test",
      "afterEach",
      "typeformEmbed",
      "jest",
      "expect",
      "XMLHttpRequest"
    ],
    "ignore": [
      "/data/js/index.js/content.js"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn dev",
    "ignore": [
      "dist/*",
      "data/*",
      "logs/*"
    ],
    "delay": 1000
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/cypress/"
    ],
    "preset": "jest-dynalite"
  }
}
