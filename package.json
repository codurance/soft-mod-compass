{
  "name": "codurance-compass",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev": ". ./scripts/default-env.sh && webpack --mode=development && npm start",
    "watch": "nodemon",
    "start": "node index.js",
    "test": "jest && npm run lint",
    "lint": "standard",
    "deploy:hubspot-dev": ". ./scripts/default-env.sh && yarn deploy:hubspot",
    "deploy:hubspot": "node ./src/hubspot/index.js"
  },
  "author": "Codurance",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "ejs": "2.6.1",
    "express": "4.16.4",
    "jsreport": "2.2.0",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "jest": "23.6.0",
    "nock": "10.0.2",
    "nodemon": "1.18.6",
    "standard": "12.0.1",
    "supertest": "3.3.0",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "typeformEmbed",
      "jest",
      "expect",
      "XMLHttpRequest"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn dev",
    "ignore": [
      "dist/*",
      "data/*",
      "logs/*"
    ],
    "delay": 1000
  }
}
