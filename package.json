{
  "name": "codurance-compass",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "dev:en": ". ./scripts/envvars-config-dev-EN.sh && webpack --mode=development && npm start",
    "dev:es": ". ./scripts/envvars-config-dev-ES.sh && webpack --mode=development && npm start",
    "debug:en": ". ./scripts/envvars-config-dev-EN.sh && webpack --mode=development && node --inspect index.js",
    "debug:es": ". ./scripts/envvars-config-dev-ES.sh && webpack --mode=development && node --inspect index.js",
    "watch": "nodemon",
    "start": "babel-node index.js",
    "test": "jest && npm run lint",
    "test:e2e": "cypress run",
    "lint": "standard",
    "check-aws-region:prod": "./scripts/check-aws-region.sh eu-central-1",
    "check-aws-region:dev": "./scripts/check-aws-region.sh eu-west-1",
    "create-env:dev:en": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-EN.sh && ./scripts/aws/create-env.sh dev-en",
    "create-env:dev:es": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-ES.sh && ./scripts/aws/create-env.sh dev-es",
    "create-env:prod:en": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-EN.sh && ./scripts/aws/create-env.sh prod-en",
    "create-env:prod:es": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-ES.sh && ./scripts/aws/create-env.sh prod-es",
    "destroy-env:dev:en": "npm run check-aws-region:dev && ./scripts/aws/destroy-env.sh dev-en",
    "destroy-env:dev:es": "npm run check-aws-region:dev && ./scripts/aws/destroy-env.sh dev-es",
    "destroy-env:prod:en": "npm run check-aws-region:prod && ./scripts/aws/destroy-env.sh prod-en",
    "destroy-env:prod:es": "npm run check-aws-region:prod && ./scripts/aws/destroy-env.sh prod-es",
    "deploy:dev:en": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-EN.sh && ./scripts/aws/deploy.sh dev-en",
    "deploy:dev:es": "npm run check-aws-region:dev && . ./scripts/envvars-config-dev-ES.sh && ./scripts/aws/deploy.sh dev-es",
    "deploy:prod:en": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-EN.sh && ./scripts/aws/deploy.sh prod-en",
    "deploy:prod:es": "npm run check-aws-region:prod && . ./scripts/envvars-config-prod-ES.sh && ./scripts/aws/deploy.sh prod-es"
  },
  "author": "Codurance",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.638.0",
    "cors": "^2.8.5",
    "cypress-iframe": "^1.0.1",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "jsreport": "2.4.0",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "sleep-promise": "8.0.1",
    "uuid": "3.3.2",
    "uuidgen": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cypress": "^4.7.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "24.3.1",
    "nock": "10.0.2",
    "nodemon": "1.18.10",
    "prettier": "2.0.5",
    "request-debug": "^0.2.0",
    "standard": "12.0.1",
    "supertest": "3.3.0",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "test",
      "afterEach",
      "typeformEmbed",
      "jest",
      "expect",
      "XMLHttpRequest"
    ],
    "ignore": [
      "/data/js/index.js/content.js"
    ]
  },
  "nodemonConfig": {
    "exec": "yarn dev",
    "ignore": [
      "dist/*",
      "data/*",
      "logs/*"
    ],
    "delay": 1000
  }
}
